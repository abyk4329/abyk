{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "pnpm: dev",
      "type": "shell",
      "command": "pnpm",
      "args": ["dev"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "pnpm: dev:v2",
      "type": "shell",
      "command": "pnpm",
      "args": ["dev:v2"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "pnpm: build",
      "type": "shell",
      "command": "pnpm",
      "args": ["build"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "pnpm: build:analyze",
      "type": "shell",
      "command": "pnpm",
      "args": ["build:analyze"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "pnpm: start",
      "type": "shell",
      "command": "pnpm",
      "args": ["start"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "pnpm: lint",
      "type": "shell",
      "command": "pnpm",
      "args": ["lint"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": ["$eslint-stylish"]
    },
    {
      "label": "pnpm: lint:fix",
      "type": "shell",
      "command": "pnpm",
      "args": ["lint:fix"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": ["$eslint-stylish"]
    },
    {
      "label": "pnpm: typecheck",
      "type": "shell",
      "command": "pnpm",
      "args": ["typecheck"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "pnpm: test:e2e",
      "type": "shell",
      "command": "pnpm",
      "args": ["test:e2e"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "test",
      "problemMatcher": []
    },
    {
      "label": "pnpm: test:e2e:smoke",
      "type": "shell",
      "command": "pnpm",
      "args": ["test:e2e:smoke"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "test",
      "problemMatcher": []
    },
    {
      "label": "Open Flare Dev Panel",
      "type": "shell",
      "command": "echo",
      "args": [
        "Use Cmd+Shift+P and search 'Joyride: Run Workspace Script', then select 'scripts/flare.cljs'"
      ],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "new"
      },
      "problemMatcher": []
    }
  ]
}
